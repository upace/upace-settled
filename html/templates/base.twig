<!doctype html>
<!--[if IE 8]><html class="no-js lt-ie10 lt-ie9" lang="en"> <![endif]-->
<!--[if IE 9]><html class="no-js lt-ie10" lang="en"> <![endif]-->
<!--[if gt IE 10]><!--><html class="no-js" lang="en"><!--<![endif]-->

<head prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb#">

    {% block head_title %}
        <title>{% if metadata.title %}{{ metadata.title }}{% else %}{{ site.name }}{% endif %}</title>
    {% endblock head_title %}

    {% block head_meta %}
        <meta charset="utf-8">
        <meta name="Content-Type" content="text/html" />
        {% if metadata.description %}
            <meta name="description" itemprop="description" content="{{ metadata.description }}">
        {% endif %}
        {% if metadata.keywords %}
            <meta name="keywords" content="{{ metadata.keywords }}">
        {% endif %}
        {% if metadata.canonical %}
            <link rel="canonical" href="{{ metadata.canonical }}">
        {% endif %}
        <meta name="ROBOTS" content="NOODP, NOYDIR" />
        <meta property="fb:app_id" content="{{ site.fb_app_id }}" />
        <meta property="og:site_name" content="{{ site.name }}" />
    {% endblock head_meta %}

    {% block meta_viewport %}
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% endblock meta_viewport %}

    {% block extra_meta %}{% endblock extra_meta %}

    {% block favicons %}
        <link rel="apple-touch-icon" sizes="57x57" href="{{ site.images_path }}/icons/apple-touch-icon-57x57.png">
        <link rel="apple-touch-icon" sizes="60x60" href="{{ site.images_path }}/icons/apple-touch-icon-60x60.png">
        <link rel="apple-touch-icon" sizes="72x72" href="{{ site.images_path }}/icons/apple-touch-icon-72x72.png">
        <link rel="apple-touch-icon" sizes="76x76" href="{{ site.images_path }}/icons/apple-touch-icon-76x76.png">
        <link rel="apple-touch-icon" sizes="114x114" href="{{ site.images_path }}/icons/apple-touch-icon-114x114.png">
        <link rel="apple-touch-icon" sizes="120x120" href="{{ site.images_path }}/icons/apple-touch-icon-120x120.png">
        <link rel="apple-touch-icon" sizes="144x144" href="{{ site.images_path }}/icons/apple-touch-icon-144x144.png">
        <link rel="apple-touch-icon" sizes="152x152" href="{{ site.images_path }}/icons/apple-touch-icon-152x152.png">
        <link rel="apple-touch-icon" sizes="180x180" href="{{ site.images_path }}/icons/apple-touch-icon-180x180.png">
        <link rel="icon" type="image/png" href="{{ site.images_path }}/icons/favicon-32x32.png" sizes="32x32">
        <link rel="icon" type="image/png" href="{{ site.images_path }}/icons/favicon-194x194.png" sizes="194x194">
        <link rel="icon" type="image/png" href="{{ site.images_path }}/icons/favicon-96x96.png" sizes="96x96">
        <link rel="icon" type="image/png" href="{{ site.images_path }}/icons/android-chrome-192x192.png" sizes="192x192">
        <link rel="icon" type="image/png" href="{{ site.images_path }}/icons/favicon-16x16.png" sizes="16x16">
        <link rel="manifest" href="{{ site.images_path }}/icons/android-chrome-manifest.json">
        <link rel="shortcut icon" href="{{ site.images_path }}/icons/favicon.ico">
        <meta name="msapplication-TileColor" content="#00aba9">
        <meta name="msapplication-TileImage" content="{{ site.images_path }}/icons/mstile-144x144.png">
        <meta name="msapplication-config" content="{{ site.images_path }}/icons/browserconfig.xml">
        <meta name="theme-color" content="#ffffff">
    {% endblock favicons %}
	
	{% block parse %}
		<script src="{{ site.js_path }}/libs/parse-1.2.12.min.js"></script>
		<script type="text/javascript">
			Parse.initialize('{{ site.parse_app_id }}', '{{ site.parse_js_key }}');
			(function() {
				var anonAllowed = !!('{{ anonymousUsersAllowed }}'), currentUser = Parse.User.current();
				if (!currentUser && !anonAllowed) window.location = '/login' ;
				else if (currentUser && anonAllowed) window.location = '/';
			})();
		</script>
	{% endblock parse %}

    {% block stylesheets %}
        <link href="http://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.1/css/bootstrap.min.css" rel="stylesheet" type="text/css">
        <link href="http://cdnjs.cloudflare.com/ajax/libs/bootstrap-switch/3.3.1/css/bootstrap3/bootstrap-switch.min.css" rel="stylesheet" type="text/css">
        <link href="{{ site.css_path }}/trunk.css" rel="stylesheet" type="text/css">
        <link href="{{ site.css_path }}/reset.css" rel="stylesheet" type="text/css">
        <link href="{{ site.css_path }}/global.css" rel="stylesheet" type="text/css">
        <link href="{{ site.css_path }}/owl.carousel.css" rel="stylesheet" type="text/css">
        <link href="http://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
        <link href='http://fonts.googleapis.com/css?family=Montserrat:400,700' rel="stylesheet" type="text/css">
        <link href='http://fonts.googleapis.com/css?family=Questrial' rel="stylesheet" type="text/css">
    {% endblock stylesheets %}

    {% block extra_css %}{% endblock extra_css %}

    <script type="text/javascript">
        window.site = {
            {% for key, value in site %}
            '{{ key }}' : '{{ value }}'{% if not loop.last %},{% endif %}
            {% endfor %}
        };
    </script>

    {% block extra_js %}{% endblock extra_js %}

</head>

<body id="page-top" class="{% block body_class %}{% endblock body_class %}" itemscope itemtype="http://schema.org/WebPage">

{% block header %}{% endblock header %}

{% block content %}{% endblock content %}

{% block footer %}{% endblock footer %}

    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.1/js/bootstrap.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/bootstrap-switch/3.3.1/js/bootstrap-switch.min.js"></script>
    <script src="{{ site.js_path }}/trunk.js"></script>
    <script src="{{ site.js_path }}/libs/owl.carousel.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/twig.js/0.6.0/twig.js"></script>
    <script src="{{ site.js_path }}/upace-api.js"></script>
    <script src="{{ site.js_path }}/upace-ui.js"></script>

{% block extra_footer_js %}{% endblock extra_footer_js %}

</body>
</html>
